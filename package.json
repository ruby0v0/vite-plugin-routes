{
  "name": "vite-plugin-routes",
  "type": "module",
  "version": "1.0.0",
  "description": "Automatically generate a vite plugin for vue-router",
  "author": "ruby0v0",
  "license": "MIT",
  "keywords": [
    "vite",
    "vue",
    "vue-router",
    "plugin",
    "routes"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c --watch --watch.exclude examples/**",
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "NODE_OPTIONS='--no-warnings' eslint",
    "lint:fix": "NODE_OPTIONS='--no-warnings' eslint --fix",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.1",
    "rollup": "^4.52.4",
    "vite": "^7.1.9",
    "vue": "^3.5.22",
    "vue-router": "^4.6.0"
  }
}
